package pw;

public class Test01_OutPut_Array {

	public static void main(String[] args) {

		int arr[] = new int[7];

		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		arr[3] = 40;
		arr[4] = 50;

		
		System.out.println(arr[0]);
		System.out.println(arr[1]);
		System.out.println(arr[2]);
		
		for(int i=0;i<arr.length;i++) {
		System.out.println(arr[i]);	
		}
		
	}

}
