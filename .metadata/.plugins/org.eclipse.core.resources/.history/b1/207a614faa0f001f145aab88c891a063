package design_pattern_creational_singleton;

class B {

	static B b = null;

	private B() {

	}
	
	public static B getRef() {
		return b;
	}

}

public class Test1 {
	public static void main(String[] args) {
              System.out.println(B.getRef());
              System.out.println(B.getRef());
              System.out.println(B.getRef());
              
	}
}
