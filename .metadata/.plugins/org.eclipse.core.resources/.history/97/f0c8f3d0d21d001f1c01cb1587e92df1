package immutable_class_practice_Selenium_Express;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collector;
import java.util.stream.Collectors;

//I need to return largest word

public class Test {

	public static void main(String[] args) {
		String str = "I am Java Developer Develope2";
		
		
		String[] str1 = str.split(" ");
		
		// step - 1 find largest word
		
		
		//step -2
		
		Map<String, Integer> mapAns=Arrays.asList(str1).stream().filter(word->word.length()>max).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
		
		
		mapAns.forEach((key,value)->System.out.println);
		
		
		
		
	}
	
	
}
