	public static int count_maxvalue(String[] str){
		int max_count = 0;
	for(int i=0;i<str.length;i++){
						  
						  if(str[i]!=str[i+1]) {
						        i++;
								max_count   = i;
								continue;
						  }else
						  {
						       count_max = 0; 
						       break;
						  }
						  
					  }

	public static void main(String[] args) {
		String s = "abcabcbb";
		String[] str = s.split("");
	}

